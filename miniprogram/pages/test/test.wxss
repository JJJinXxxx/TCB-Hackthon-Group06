

.cover{
  width: 100vw;
  height: 100vh;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.outcome{
  width: 93%;
  height: 95%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  border-radius: 30px;
}

.card{
  width: 93%;
  height: 95%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 50rpx;
  box-sizing: border-box;

  background-color:#fff;
	box-shadow: 10px 10px 5px  #888888;
  border-radius: 20px;
  
  color: #DAE1DA;
  letter-spacing:10rpx;
  font-weight: 1000;
  font-size: 100px;
} 

.start{
  position: absolute;
  top: 65%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size: 1.2em;
  width: 50vw!important;
}
.finnal{
  position: absolute;
  top: 63%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size: 1.2em;
  width: 50vw!important;
  background:#3b9a9c3f;

}
.goal{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size: 1.2em;
  background: rgb(255, 255, 255);
  color:#3b9a9c;
  margin: 10px;
  width: 300px!important;
}


.question{
  display: flex;
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 70vw;
  height: 30vh;
  align-items: center;
  text-align: left;
  font-size: 0.3em;
  border-radius: 30px;
  background: rgb(235, 243, 235);
  color:#3b9a9c;
  margin-bottom: 4vh;
}

button{
  font-size: 0.18em;
  margin-bottom: 4vh;
  /* justify-content: space-between; */
  border-radius: 10px;
  background: rgb(235, 243, 235);
  color:#3b9a9c;
  letter-spacing:2rpx;
  width: 70vw!important;
}
