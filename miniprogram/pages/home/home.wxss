
image{
  width: 100px;
  height: 100px;
  border-radius: 50px;
  margin: 10px;
}
.team{
  width: 93%;
  height: 95%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 50rpx;
  box-sizing: border-box;

  background-color:#fff;
	box-shadow: 10px 10px 5px  #888888;
  border-radius: 20px;
  
  color:#3b9a9c;
  font-weight: 1000;
  font-size: 2.2em;
} 
